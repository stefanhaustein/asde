ASDE
10 LET bob = screen.newSprite()
20 bob.size = 50
30 LET dx = 0.5
40 LET dy = 0
50 bob.yAlign = YAlign.BOTTOM
60 bob.xAlign = XAlign.LEFT
100 if bob.x < 0 then dx = 1
110 if bob.x > screen.width - bob.size then dx = -1
120 if bob.y < 0 then dy = 5
130 bob.x = bob.x + dx
140 bob.y = bob.y + dy
150 bob.angle = bob.angle + 2 * dx
160 dy = dy - 0.1
170 sleep 15
180 goto 100
