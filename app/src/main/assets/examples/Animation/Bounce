ASDE
10 var bob = screen.newSprite()
20 bob.size = 50
30 var dx = 0.5
40 var dy = 0
50 bob.yAlign = YAlign.BOTTOM
60 bob.xAlign = XAlign.LEFT
99 while true:
100 if bob.x < 0:
101   dx = 1
102 end
110 if bob.x > screen.width - bob.size:
111   dx = -1
112 end
120 if bob.y < 0: dy = 5; end
130 bob.x = bob.x + dx
140 bob.y = bob.y + dy
150 bob.angle = bob.angle + 2 * dx
160 dy = dy - 0.1
170 sleep 15
180 end
